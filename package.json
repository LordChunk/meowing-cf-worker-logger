{
  "name": "meowing-cf-dashboard",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve & yarn run serve:api",
    "start": "ng serve",
    "build": "ng build --build-optimizer",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:api": "dotnet run --project ../meowing-cf-logger-api/API",
    "generate-api-service": "ng-openapi-gen --input https://meowing-api.chu.mk/swagger/v1/swagger.json --output ./src/services/api --skipJsonSuffix true",
    "dev:ssr": "ng run meowing-cf-dashboard:serve-ssr",
    "serve:ssr": "node dist/meowing-cf-dashboard/server/main.js",
    "build:ssr": "ng build && ng run meowing-cf-dashboard:server",
    "prerender": "ng run meowing-cf-dashboard:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.0",
    "@angular/cdk": "~12.1.0",
    "@angular/common": "~12.1.0",
    "@angular/compiler": "~12.1.0",
    "@angular/core": "~12.1.0",
    "@angular/forms": "~12.1.0",
    "@angular/material": "~12.1.0",
    "@angular/platform-browser": "~12.1.0",
    "@angular/platform-browser-dynamic": "~12.1.0",
    "@angular/platform-server": "~12.1.0",
    "@angular/router": "~12.1.0",
    "@nguniversal/express-engine": "12.1.0",
    "chart.js": "^3.3.2",
    "chartjs-chart-treemap": "^1.0.1",
    "express": "^4.15.2",
    "ng2-charts": "^3.0.0-rc.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.0",
    "@angular/cli": "~12.1.0",
    "@angular/compiler-cli": "~12.1.0",
    "@nguniversal/builders": "^12.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.8.1",
    "@types/node": "^15.12.4",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "typescript": "~4.2.3"
  }
}